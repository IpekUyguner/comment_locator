<idea-plugin>
    <id>org.jetbrains.plugins.template</id>
    <name>Template</name>
    <vendor>JetBrains</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.clion</depends>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.cidr.debugger</depends>
    <depends>com.intellij.cidr.base</depends>
    <depends>com.intellij.modules.lang</depends>


    <extensions defaultExtensionNs="com.intellij">
        <localInspection  enabledByDefault="true" level="WARNING" runForWholeFile="true"
                          shortName="Commentor" displayName="CommentLocator" groupName="Inspection"
                          implementationClass="PredictorInspection"/>
    </extensions>

</idea-plugin>
